type CanisterInfo = record {
  id : text;
  created : int;
  name : text;
  parent_name : text;
};
type FileScalingManager = service {
  get_canister_records : () -> (vec CanisterInfo) query;
  get_file_storage_canister_id : () -> (text) query;
  init : () -> (text);
  version : () -> (nat) query;
};
service : (bool) -> FileScalingManager